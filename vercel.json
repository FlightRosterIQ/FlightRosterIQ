{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/roster-updates",
      "destination": "/api/roster-updates"
    },
    {
      "source": "/api/:path*",
      "destination": "https://flightrosteriq.onrender.com/api/:path*"
    }
  ]
}
